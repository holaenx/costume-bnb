<h1>Create your costume offer</h1>
<%= simple_form_for @costume do |f| %>
  <%= f.input :name %>
  <%= f.input :description%>
  <%= f.input :size, placeholder:"XS,S,M,L,XL,XXL"%>
  <%= f.input :city%>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
